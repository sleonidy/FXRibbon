<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<BorderPane id="BorderPane" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="514.0" prefWidth="410.0">
    <top>
        <Ribbon>
            <tabs>
                <RibbonTab text="Home">
                    <ribbonGroups>
                        <RibbonGroup title="File">
                            <nodes>
                                <Button contentDisplay="TOP" text="Open">
                                    <graphic>
                                        <ImageView>
                                            <Image url="@icons8_Open_32px_3.png" />
                                        </ImageView>
                                    </graphic>
                                </Button>
                                <Button contentDisplay="TOP" text="New">
                                    <graphic>
                                        <ImageView>
                                            <Image url="@icons8_File_32px_1.png" />
                                        </ImageView>
                                    </graphic>
                                </Button>
                                <Button contentDisplay="TOP" text="Save">
                                    <graphic>
                                        <ImageView>
                                            <Image url="@icons8_Save_32px.png" />
                                        </ImageView>
                                    </graphic>
                                </Button>
                                <Button contentDisplay="TOP" text="Save as">
                                    <graphic>
                                        <ImageView>
                                            <Image url="@icons8_Save_as_32px.png" />
                                        </ImageView>
                                    </graphic>
                                </Button>
                            </nodes>
                        </RibbonGroup>
                    </ribbonGroups>
                </RibbonTab>
            </tabs>
        </Ribbon>

    </top>
</BorderPane>
